{
  "name": "emretepedev.github.io",
  "description": "Personal Homepage.",
  "version": "1.0.0",
  "author": {
    "name": "Emre Tepe",
    "email": "emretepedev@gmail.com",
    "url": "https://emretepedev.github.io/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "analyze": "nuxt build --analyze",
    "format": "prettier '**/{*,''}.{css,scss,json,js,vue}' --write; eslint '**/{*,''}.{json,js,vue}' --fix; stylelint '**/*.{css,scss,vue}' --fix;",
    "format:css": "prettier '**/*.css' --write; stylelint '**/*.css' --fix;",
    "format:scss": "prettier '**/*.scss' --write; stylelint '**/*.scss' --fix;",
    "format:json": "prettier '**/{*,''}.json' --write; eslint '**/{*,''}.json' --fix;",
    "format:js": "prettier '**/{*,''}.js' --write; eslint '**/{*,''}.js' --fix;",
    "format:vue": "prettier '**/*.vue' --write; eslint '**/*.vue' --fix; stylelint '**/*.vue' --fix;",
    "lint": "cspell lint --gitignore --dot '**/{*,''}{.,''}*'; prettier '**/{*,''}.{css,scss,json,js,vue}' --check; eslint '**/{*,''}.{json,js,vue}'; stylelint '**/*.{css,scss,vue}';",
    "fetch": "tsc contributionsCount.ts && node contributionsCount.js",
    "hook:pre-push": "cspell lint --gitignore --dot '**/{*,''}{.,''}*' && prettier '**/{*,''}.{css,scss,json,js,vue}' --check && eslint '**/{*,''}.{json,js,vue}' && stylelint '**/*.{css,scss,vue}'",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run hook:pre-push"
    }
  },
  "resolutions": {
    "@nuxt/kit": "3.0.0-rc.13"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "@metamask/detect-provider": "^2.0.0",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/recaptcha": "^1.1.1",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "axios": "^1.2.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
    "nuxt": "^2.5.18",
    "vee-validate": "^3.4.14",
    "vue-gtag": "^1.16.1",
    "vue-long-click": "^0.1.0",
    "vue-toastify": "^1.8.1",
    "web3": "^1.8.1"
  },
  "devDependencies": {
    "@nuxt/image": "0.7.1",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/google-fonts": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/node": "^16.0.0",
    "cspell": "^6.17.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "fs": "^0.0.1-security",
    "husky": "^8.0.2",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "push-dir": "^0.4.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
