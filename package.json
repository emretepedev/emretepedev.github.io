{
  "name": "emretepedev",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "prepare": "husky install; rm -rf .husky/pre-push; husky add .husky/pre-push 'yarn run hook:pre-push'; rm -rf .husky/_/.gitignore",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "analyze": "nuxt build --analyze",
    "format": "eslint '**/*.{json,js,vue}' --fix; stylelint '**/*.{css,scss,vue}' --fix; prettier '**/*.{css,scss,json,js,vue}' --write;",
    "format:css": "stylelint '**/*.css' --fix; prettier '**/*.css' --write;",
    "format:scss": "stylelint '**/*.scss' --fix; prettier '**/*.scss' --write;",
    "format:json": "eslint '**/*.json' --fix; prettier '**/*.json' --write;",
    "format:js": "eslint '**/*.js' --fix; prettier '**/*.js' --write;",
    "format:vue": "eslint '**/*.vue' --fix; stylelint '**/*.vue' --fix; prettier '**/*.vue' --write;",
    "lint": "eslint '**/*.{json,js,vue}'; stylelint '**/*.{css,scss,vue}'; prettier '**/*.{css,scss,json,js,vue}' --check;",
    "fetch": "tsc --project . && node contributionsCount.js",
    "hook:pre-push": "eslint '**/*.{json,js,vue}' && stylelint '**/*.{css,scss,vue}' && prettier '**/*.{css,scss,json,js,vue}' --check",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run hook:pre-push"
    }
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@metamask/detect-provider": "^1.2.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.31.0",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/recaptcha": "^1.0.4",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "nuxt": "^2.15.8",
    "vee-validate": "^3.4.14",
    "vue-gtag": "^1.16.1",
    "vue-toastify": "^1.8.1",
    "web3": "^1.7.0"
  },
  "devDependencies": {
    "@nuxt/image": "0.6.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/tailwindcss": "^5.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "fs": "^0.0.1-security",
    "husky": "^7.0.4",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "8",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "push-dir": "^0.4.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.5"
  }
}
