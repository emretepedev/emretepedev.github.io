{
  "name": "emretepedev.github.io",
  "description": "Personal Homepage.",
  "version": "1.0.0",
  "author": {
    "name": "Emre Tepe",
    "email": "emretepedev@gmail.com",
    "url": "https://emretepedev.github.io/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider nuxi dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider nuxi generate",
    "start": "NODE_OPTIONS=--openssl-legacy-provider nuxi preview",
    "output": "NODE_OPTIONS=--openssl-legacy-provider nuxi prepare",
    "analyze": "NODE_OPTIONS=--openssl-legacy-provider nuxt build --analyze",
    "format": "prettier '**/{*,''}.{css,json,js,vue}' --write; eslint '**/{*,''}.{json,js,vue}' --fix; stylelint '**/*.{css,vue}' --fix;",
    "format:css": "prettier '**/*.css' --write; stylelint '**/*.css' --fix;",
    "format:json": "prettier '**/{*,''}.json' --write; eslint '**/{*,''}.json' --fix;",
    "format:js": "prettier '**/{*,''}.js' --write; eslint '**/{*,''}.js' --fix;",
    "format:vue": "prettier '**/*.vue' --write; eslint '**/*.vue' --fix; stylelint '**/*.vue' --fix;",
    "lint": "cspell lint --gitignore --dot '**/{*,''}{.,''}*'; prettier '**/{*,''}.{css,json,js,vue}' --check; eslint '**/{*,''}.{json,js,vue}'; stylelint '**/*.{css,vue}';",
    "fetch": "tsc contributions.ts && node contributions.js",
    "hook:pre-push": "cspell lint --gitignore --dot '**/{*,''}{.,''}*' && prettier '**/{*,''}.{css,json,js,vue}' --check && eslint '**/{*,''}.{json,js,vue}' && stylelint '**/*.{css,vue}'",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run hook:pre-push"
    }
  },
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/recaptcha": "^1.1.1",
    "@pinia/nuxt": "^0.4.6",
    "ethers": "^5.7.2",
    "nuxt-edge": "latest",
    "vee-validate": "^3.4.14",
    "vue-gtag-next": "^1.14.0",
    "vue-toastify": "^1.8.1",
    "web3": "1.8.1"
  },
  "devDependencies": {
    "@nuxt/bridge": "npm:@nuxt/bridge-edge",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "^2.0.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/node": "^18.0.0",
    "@vueuse/components": "^9.8.2",
    "@vueuse/core": "^9.8.2",
    "@vueuse/nuxt": "^9.8.2",
    "autoprefixer": "^10.4.13",
    "cspell": "^6.17.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "fs": "^0.0.1-security",
    "husky": "^8.0.2",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "push-dir": "^0.4.1",
    "serve": "^14.1.2",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
